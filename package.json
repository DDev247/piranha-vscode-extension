{
    "name": "mantaray-sdl-extension",
    "displayName": "MantaRay SDL Extension",
    "description": "Syntax highlighting support for MantaRay SDL.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "mantascene",
            "aliases": ["MantaRay SDL", "mantascene"],
            "extensions": [".mr"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mantascene",
            "scopeName": "source.mr",
            "path": "./syntaxes/mantascene.tmLanguage.json"
        }]
    }
}
{
    "name": "piranha-extension",
    "displayName": "Piranha Extension",
    "description": "Syntax highlighting support for Piranha.",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "piranha",
            "aliases": ["Piranha"],
            "extensions": [".mr", ".pr"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mantascene",
            "scopeName": "source.mr",
            "path": "./syntaxes/piranha.tmLanguage.json"
        }]
    }
}
